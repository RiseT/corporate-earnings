{
    "manifest_version": 2,
    "name": "Corporate Earnings",
    "version": "0.1",
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches":[
                "https://seekingalpha.com/symbol/*/earnings"
            ],
            "js":["jquery-3.4.0.min.js", "content.js"],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}